{
  "name": "@gogoing/javascript-utils",
  "version": "0.0.4",
  "author": {
    "name": "Frank",
    "email": "1056616012@qq.com"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gavinkd/javascript-utils.git"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "prepare": "husky install",
    "dev": "npm run clear && rollup -c -w",
    "build": "npm run clear && rollup -c",
    "clear": "rimraf dist",
    "test": "jest test/",
    "lint": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^4.27.0",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.5",
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-eslint": "^8.0.1",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "commitizen": "^4.2.5",
    "coveralls": "^3.1.1",
    "husky": "^7.0.4",
    "jest": "^29.3.1",
    "jest-config": "^29.3.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.3.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "typedoc": "^0.23.21",
    "typescript": "^4.3.4"
  },
  "dependencies": {},
  "hooks": {
    "pre-commit": "npm test"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/*.{js,ts}"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
